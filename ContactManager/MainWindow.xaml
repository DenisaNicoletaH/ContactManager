        <Label Content="Contact Manager" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top" FontSize="24px"/>
        <ListView x:Name="listViewList">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
